class Mol
{
   void mul(int a,int b)
   {
      System.out.println("Multiplication is "+(a*b));
   }
   void mul(int a,int b,int c)
   {
      System.out.println("multiplication is "+(a*b*c));
   }
   public static void main(String args[])
   {
      Mol m=new Mol();
      m.mul(2,3);
      m.mul(2,5,6);
   }
} 
